programa {
	funcao inicio() {
	    inteiro a, b, c

		escreva("Digite sua idade: Informe quantos anos: ")
		leia(a)
		
		a = a*365
		
		escreva("Informe quantos meses: ")
		leia(b)
		
		b=b*30
		
		escreva("Informe quantos dias: ")
		leia(c)
		
		c = a+b+c
		
		escreva("Você tem ", c," dias de vida")
	}
}
